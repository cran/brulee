Package: brulee
Title: High-Level Modeling Functions with 'torch'
Version: 0.0.1
Authors@R: c(
    person("Max", "Kuhn", , "max@rstudio.com", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0003-2402-136X")),
